{
  "name": "@sck/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:clean": "lsof -ti:3000 | xargs kill -9 2>/dev/null || true && next dev -p 3000",
    "dev:force": "./scripts/start-dev.sh",
    "dev:ready": "npm run check:db && npm run prisma:generate && npm run dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "postinstall": "echo 'Skipping postinstall in CI' || prisma generate --schema=./prisma/schema.prisma --no-engine",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "db:generate": "prisma generate --schema=./prisma/schema.prisma",
    "check:db": "node ./scripts/check-db.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@magic-ext/oauth": "^23.2.1",
    "@prisma/client": "^6.12.0",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@types/node": "^20.19.8",
    "@types/vis": "^4.21.27",
    "@wagmi/core": "^2.17.3",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "ethers": "^6.15.0",
    "express": "^5.1.0",
    "lucide-react": "^0.294.0",
    "magic-sdk": "^29.2.1",
    "next": "^15.4.1",
    "next-auth": "^4.24.7",
    "pg": "^8.16.3",
    "pino-pretty": "^13.0.0",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-toastify": "^11.0.5",
    "recharts": "^3.1.0",
    "tailwind-merge": "^2.1.0",
    "tailwindcss": "^3.3.6",
    "tailwindcss-animate": "^1.0.7",
    "viem": "^2.32.0",
    "vis-network": "^10.0.1",
    "wagmi": "^2.15.7",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
