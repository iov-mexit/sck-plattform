# SCK Security Framework Dataset Ingestion Environment Variables
# Copy this file to .env and fill in your actual values

# OpenAI API Configuration
OPENAI_API_KEY=your-openai-api-key-here

# Pinecone Vector Database Configuration  
PINECONE_API_KEY=your-pinecone-api-key-here
PINECONE_INDEX=sck-knowledge

# Database Configuration (if using Prisma)
DATABASE_URL=postgresql://username:password@localhost:5432/sck_database

# Optional: Pinecone Environment (e.g., us-west1-gcp)
PINECONE_ENVIRONMENT=us-west1-gcp
