{
  "name": "sck-monorepo",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@4.0.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "db:generate": "turbo run db:generate",
    "db:push": "turbo run db:push",
    "db:studio": "turbo run db:studio",
    "vercel-build": "yarn build",
    "vercel-dev": "yarn dev",
    "vercel-start": "yarn start",
    "deploy:vercel": "vercel --prod",
    "deploy:preview": "vercel",
    "setup:production": "yarn db:generate && yarn build",
    "health:check": "curl -f http://localhost:3000/api/v1/health || exit 1"
  },
  "devDependencies": {
    "turbo": "^2.5.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iov-mexit/sck-plattform.git"
  },
  "keywords": [
    "sck",
    "platform",
    "role-agents",
    "ans",
    "blockchain",
    "trust",
    "vercel"
  ],
  "author": "SCK Team",
  "license": "MIT"
}
